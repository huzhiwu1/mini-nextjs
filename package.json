{
  "name": "mini-nextjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config webpack.client.js",
    "build": "webpack --config webpack.server.js && webpack --config webpack.client.js",
    "watch-client": "webpack --config webpack.client.js --watch",
    "watch-server": "webpack --config webpack.server.js --watch",
    "dev": "npm run build && npm run watch-server & npm run watch-client & nodemon build/server.bundle.js --watch build/server.bundle.js",
    "start-server": "webpack --config webpack.server.js && webpack --config webpack.client.js && node build/server.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huzhiwu1/mini-nextjs.git"
  },
  "author": "",
